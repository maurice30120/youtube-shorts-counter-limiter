{
  "manifest_version": 3,
  "name": "YouTube Shorts Counter",
  "version": "1.3.1",
  "description": "Compte le nombre de YouTube Shorts visit√©s",
  "permissions": [
    "webNavigation",
    "storage",
    "tabs",
    "notifications"
  ],
"background": {
  "scripts": ["background.js"]
},
  "action": {
    "default_popup": "popup.html",
    "default_title": "YouTube Shorts Counter",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
    }
  }
}